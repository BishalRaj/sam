<div class="container">
    <h1>total: <%= total %>
    </h1>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Date</th>
                <th scope="col">Company</th>
                <th scope="col">Pan number</th>
                <th scope="col">Bill</th>
                <th scope="col">Item</th>
                <th scope="col">Vatable Amount</th>
                <th scope="col">Taxable</th>
                <th scope="col">Total Amount</th>
            </tr>
        </thead>
        <tbody>


            <% var id=1; data.forEach(function(bill){ %>
                <tr>
                    <th scope="row">
                        <%= id %>
                    </th>
                    <td>
                        <%= bill.date %>
                    </td>
                    <td>
                        <%= bill.particular %>
                    </td>
                    <td>
                        <%= bill.pan %>
                    </td>
                    <td>
                        <%= bill.bill %>
                    </td>
                    <td>
                        <%= bill.item %>
                    </td>
                    <td>
                        <%= bill.vatable %>
                    </td>
                    <td>
                        <%= bill.taxable %>
                    </td>
                    <td>
                        <%= bill.total %>
                    </td>
                </tr>
                <% id++; }) %>


        </tbody>
    </table>


</div>